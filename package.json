{
  "name": "chushpan-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev:vk": "nodemon --delay 500ms -L src/vk.ts",
    "dev:tg": "nodemon --delay 500ms -L src/tg.ts",
    "dev:ds": "nodemon --delay 500ms -L src/ds.ts",
    "test": "jest",
    "migrate:up:latest:dev": "export NODE_ENV=DEVELOPMENT && ts-node src/db/migrator.ts upLatest",
    "migrate:down:last:dev": "export NODE_ENV=DEVELOPMENT && ts-node src/db/migrator.ts down",
    "format": "yarn prettier . --write"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "kysely": "^0.27.3",
    "node-schedule": "^2.1.1",
    "pg": "^8.11.3",
    "rimraf": "^5.0.5",
    "telegraf": "^4.16.3",
    "vk-io": "^4.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.28",
    "@types/node-schedule": "^2.1.6",
    "@types/pg": "^8.11.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
